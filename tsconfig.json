{
	"compilerOptions": {
		"watch": true,
		"target": "ES2020",
		"allowJs": true,
		"module": "ES2020",
		"resolveJsonModule": true,
		"lib": [
			"ES2020"
		],
		"esModuleInterop": true,
		"moduleResolution": "node",
		"outDir": "./dist/",
		"sourceMap": false,
	},
	"include": [
		"src/**/*.*",
		"src/json/memory.json",
		"src/json/.env"
	]
}